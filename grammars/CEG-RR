Goal   : Expr 
       ;

Expr   : Term EPrime 
       ;

EPrime : PLUS  Term EPrime
       | MINUS Term EPrime
       | epsilon 
       ;

Term   : Factor TPrim
       ;

TPrime : TIMES Factor TPrime
       |  DIV   Factor TPrime
       | epsilon 
       ;

Factor : LP Expr RP
       | NUMBER
       | IDENTIFIER
       ;

